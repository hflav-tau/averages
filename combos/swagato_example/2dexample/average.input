OUTPUT 6 ! logical unit for log file
       6 ! logical unit for warning and error messages 
      10 ! logical unit for output cards file

INCLUDE analysis1.input
INCLUDE analysis2.input

BEGIN   A2 2D_XY PUBLISHED
COMBINE A2 *     PUBLISHED
MEASUREMENT X stat syst
MEASUREMENT Y stat syst
CALL DUMP_MASTER_INC
PARAMETER CHI2_N_SYM_PRT -1.0 0. 0.
PARAMETER A2_SYST1 1. 1. -1.
CALL CHI2_N_SYM
* SWITCH STAT_CORR OFF
* SWITCH SYST_CORR OFF
END

BEGIN   A1 2D_XY PUBLISHED
COMBINE A1 *     PUBLISHED
MEASUREMENT X stat syst
MEASUREMENT Y stat syst
CALL DUMP_MASTER_INC
PARAMETER CHI2_N_SYM_PRT -1.0 0. 0.
PARAMETER A1_SYST1 1. 1. -1.
CALL CHI2_N_SYM
* SWITCH STAT_CORR OFF
* SWITCH SYST_CORR OFF
END

BEGIN   ALL 2D_XY PUBLISHED
COMBINE *   *     *
MEASUREMENT X stat syst
MEASUREMENT Y stat syst
CALL DUMP_MASTER_INC
PARAMETER CHI2_N_SYM_PRT -1.0 0. 0.
PARAMETER A1_SYST1 1. .8 -.8
PARAMETER A2_SYST1 1. .8 -.8
* SWITCH ADJUSTMENTS OFF
* SWITCH STAT_CORR OFF
* SWITCH SYST_CORR OFF
CALL CHI2_N_SYM
END

*BEGIN   ALL X PUBLISHED
*COMBINE *   X     *
*MEASUREMENT X stat syst
*CALL DUMP_MASTER_INC
** CALL CHI2_SYM
*PARAMETER CHI2_N_SYM_PRT 1.0 0. 0.
*PARAMETER A1_SYST1 1. 1. -1.
*PARAMETER A2_SYST1 1. 1. -1.
*CALL CHI2_N_SYM
** SWITCH STAT_CORR OFF
** SWITCH SYST_CORR OFF
*END

