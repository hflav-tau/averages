WITHOUT UNITARITY CONSTRAINT
ln -s combos_average_pdginput_with_babar_belle_unconstrained.input average.input
make combos
getresult_combos_unconstrained.csh | tee getresult_combos_unconstrained.log
less getresult_combos_unconstrained.log | awk '{print "grep \""$1" \" ../TauFit-alu/getresults_alucomb.log"}' | sh | tee getresults_alucomb_unconstrained.log
paste getresult_combos_unconstrained.log getresults_alucomb_unconstrained.log | awk '{print $0, (1-($2/$5))*100, " % " , (1-($3/$6))*100, " % "}' | tee compare_combos_alucomb_unconstrained.log
cp -p average.log log/
cp -p getresult_combos_unconstrained.log log/
cp -p getresults_alucomb_unconstrained.log log/
cp -p compare_combos_alucomb_unconstrained.log log

WITH UNITARITY CONSTRAINT
ln -s combos_average_pdginput_with_babar_belle.input average.input
make combos
getresult_combos.csh | tee getresult_combos.log
less getresult_combos.log | awk '{print "grep \""$1" \" ../TauFit-alu/getresults_alucomb.log"}' | sh | tee getresults_alucomb.log
paste getresult_combos.log getresults_alucomb.log | awk '{print $0, (1-($2/$5))*100, " % " , (1-($3/$6))*100, " % "}' | tee compare_combos_alucomb.log
cp -p average.log log/with_constraint/
cp -p getresult_combos.log log/with_constraint/
cp -p getresults_alucomb.log log/with_constraint/
cp -p compare_combos_alucomb.log log/with_constraint/

